{
  "name": "pylon",
  "version": "1.1.0",
  "description": "field_description",
  "main": "server.js",
  "scripts": {
    "start": "ts-node --transpile-only ./server.ts",
    "test": "npm run lint && npm run cover",
    "check-coverage": "nyc check-coverage --lines 65 --functions 60 --Statements 65",
    "ava": "NODE_ENV=test ava",
    "watch": "gulp watch",
    "build": "gulp build",
    "build-production": "NODE_ENV=production gulp build",
    "server": "node server",
    "lint": "pretty-quick --staged && eslint src --fix --ext .js,.tsx,.ts",
    "lint:css": "stylelint 'css/*.css' 'src/**/*.tsx' --fix",
    "postinstall": "npm run bp",
    "cover": "NODE_ENV=test tsc && nyc npm run ava",
    "size": "ls -lh dist/",
    "bp": "npm run build-production && npm run size",
    "kill": "lsof -n -i4TCP:5000 | grep LISTEN | awk '{ print $2 }' | xargs kill"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint",
      "react"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "curly": [
        "error",
        "all"
      ],
      "max-classes-per-file": 0,
      "no-param-reassign": 0,
      "no-restricted-syntax": [
        "error",
        "ForInStatement",
        "LabelStatement",
        "WithStatement"
      ],
      "no-underscore-dangle": 0,
      "no-unused-expressions": 0,
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "import/prefer-default-export": 0,
      "class-methods-use-this": 0
    },
    "overrides": [
      {
        "files": "*.js",
        "rules": {
          "@typescript-eslint/explicit-function-return-type": 0
        }
      },
      {
        "files": "*.ts",
        "parser": "@typescript-eslint/parser"
      }
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recess-order",
      "stylelint-config-prettier"
    ],
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts",
      "src/**/__tests__/**"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "cobertura"
    ],
    "extension": [
      ".tsx",
      ".ts"
    ],
    "cache": true,
    "temp-directory": "./coverage/.nyc_output"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/puncsky/template_website.git"
  },
  "keywords": [],
  "author": "field_author",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/puncsky/template_website/issues"
  },
  "homepage": "https://github.com/puncsky/template_website#readme",
  "devDependencies": {
    "@2fd/graphdoc": "2.4.0",
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "@typescript-eslint/parser": "3.4.0",
    "ava": "3.9.0",
    "awesome-typescript-loader": "5.2.1",
    "cache-loader": "4.1.0",
    "del": "5.1.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.1",
    "fancy-log": "1.3.3",
    "glob": "7.1.6",
    "gulp": "4.0.2",
    "gulp-less": "4.0.1",
    "gulp-livereload": "4.0.2",
    "gulp-nodemon": "2.5.0",
    "gulp-sass": "4.1.0",
    "husky": "4.2.5",
